{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["dom", "dom.iterable", "esnext", "ES2016", "ES2017","ES2020","ES2022", "ES5", "ES6" ],
      "allowJs": true,
      "checkJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": false,
      "noEmit": true,
      "module": "esnext",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "noUncheckedIndexedAccess": true,
      "baseUrl": "./",
      "paths": {
        "~/*": ["./src/*" , "./src/ts/*", "./assets/*"]
      }
    },
    "exclude": [
        "node_modules",
        "working-files",
        ".parcel-cache",
        "dist",
    ]
  }